{
  "manifest_version": 3,
  "name": "Web New Extension",
  "version": "1.0",
  "permissions": ["storage", "activeTab", "scripting"],
 
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["data/data-script.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "public/icons/icon16.png",
    "48": "public/icons/icon48.png",
    "128": "public/icons/icon128.png"
  }
  
}
